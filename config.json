{
  "name": "Hoymiles Client",
  "version": "0.0.1",
  "slug": "hoymiles_cli_edge",
  "description": "Read Hoymiles Data from the Cloud",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/abankowski/ha-hoymiles",
  "startup": "application",
  "boot": "auto",
  "hassio_api": false,
  "homeassistant_api": false,
  "hassio_role": "default",
  "devicetree": false,
  "services": [
    "mqtt:need"
  ],
  "ingress": false,
  "host_network": true,
  "panel_icon": "mdi:solar-power",
  "panel_title": "Hoymiles Client",
  "panel_admin": false,
  "options": {
    "HOYMILES_CLOUD_USER": "HOYMILES_USER",
    "HOYMILES_CLOUD_PASSWORD": "HOYMILES_PASSWORD",
    "HOYMILES_CLOUD_STATION_ID": "123456",
  },
  "schema": {
    "HOYMILES_CLOUD_USER": "str",
    "HOYMILES_CLOUD_PASSWORD": "password",
    "HOYMILES_CLOUD_STATION_ID": "str",
  }
}